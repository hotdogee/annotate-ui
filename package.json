{
  "name": "annotate-ui",
  "version": "0.0.1",
  "description": "Protein Annotation using Recurrent Neural Network Models",
  "productName": "ANNotate",
  "cordovaId": "in.hanl.ann",
  "author": "Han Lin <hotdogee@gmail.com>",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production http-server ./dist/pwa -c 1 -p 8583 -d false --push-state",
    "dev": "quasar dev -m pwa",
    "release": "standard-version",
    "build": "cross-env NODE_ENV=production quasar build -m pwa",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8789",
    "audit:snyk": "snyk test --json > ./test/audits/snyk.json",
    "audit:node_modules": "yarn audit --json > ./test/audits/node_modules_audit.json",
    "audit:licenses": "nlf --summary detail > ./test/audits/licenses.txt",
    "audit:lighthouse": "lighthouse http://localhost:3000 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^4.3.3",
    "@feathersjs/feathers": "^4.3.3",
    "@feathersjs/socketio-client": "^4.3.3",
    "@quasar/extras": "^1.3.2",
    "@tweenjs/tween.js": "^18.3.1",
    "add-asset-webpack-plugin": "^1.0.0",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "echarts": "^4.2.0-rc.2",
    "feathers-client": "^2.4.0",
    "feathers-vuex": "^2.0.0-pre.78",
    "flv.js": "^1.5.0",
    "git-revision-webpack-plugin": "^3.0.4",
    "hls.js": "^0.12.4",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.7.3",
    "quasar": "^1.1.5",
    "serialize-error": "^5.0.0",
    "socket.io-client": "^2.3.0",
    "v-charts": "^1.19.0",
    "v-tooltip": "^2.0.0-rc.33",
    "vue-analytics": "^5.17.2",
    "vue-i18n": "^8.14.1",
    "vue-recaptcha": "^1.2.0",
    "vueisotope": "^3.1.2",
    "vuejs-storage": "^3.0.1",
    "vuelidate": "^0.7.4",
    "vuex-persist": "^2.1.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.1.2",
    "@quasar/quasar-app-extension-icon-genie": "^1.0.2",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.10",
    "@quasar/quasar-app-extension-testing-quality": "^1.0.0-beta.8",
    "@quasar/quasar-app-extension-testing-security": "^1.0.0-alpha.8",
    "@quasar/quasar-app-extension-testing-security-antivuln": "^1.0.0-alpha.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "standard": "^14.3.1",
    "standard-version": "^7.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
